<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduler" x:Class="Login_CS4.Scheduler"
        Title="Scheduler" Height="300" Width="670">
    <Grid>

        <dxsch:SchedulerControl x:Name="scheduler" ActiveViewType="Week">
            <dxsch:SchedulerControl.FullWeekView>
                <dxsch:FullWeekView Enabled="True"/>
            </dxsch:SchedulerControl.FullWeekView>
            <dxsch:SchedulerControl.WeekView>
                <dxsch:WeekView Enabled="False"/>
            </dxsch:SchedulerControl.WeekView>
        </dxsch:SchedulerControl>

    </Grid>
</Window>
